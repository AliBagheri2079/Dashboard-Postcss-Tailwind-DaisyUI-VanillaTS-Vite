{
  "name": "dashboard-ts",
  "title": "Dashboard Typescript Project",
  "description": "This Project Template built with TypeScript/JavaScript, Babel, Vite, Webpack 5, PostCSS, Jest, ESLint, Stylelint and ...",
  "author": "Ali Bagheri",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config webpack.config.babel.js",
    "build": "webpack --progress --config webpack.config.babel.js",
    "lint:scripts": "rome check --apply .",
    "lint:styles": "stylelint ./src/**/*.css",
    "lint": "npm run lint:scripts && npm run lint:styles",
    "format:scripts": "rome format --write .",
    "format:styles": "stylelint ./src/**/*.css --fix",
    "format": "npm run format:scripts && npm run format:styles",
    "test": "jest --verbose",
    "test:watch": "npm run test -- --watchAll",
    "test:coverage": "npm run test -- --coverage",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.21.4",
    "@babel/register": "^7.21.0",
    "@types/jest": "^29.5.0",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.29.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "cssnano": "^5.1.15",
    "daisyui": "^2.50.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.0",
    "image-minimizer-webpack-plugin": "^3.8.2",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "jest": "^29.5.0",
    "lint-staged": "^13.1.2",
    "mini-css-extract-plugin": "^2.7.5",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^8.0.1",
    "rome": "12.0.0",
    "style-loader": "^3.3.2",
    "stylelint": "^15.1.0",
    "stylelint-config-standard": "^30.0.1",
    "stylelint-webpack-plugin": "^4.1.0",
    "tailwindcss": "^3.2.7",
    "terser-webpack-plugin": "^5.3.7",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "webpack": "^5.77.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "chart.js": "^4.2.1",
    "firebase": "^9.18.0",
    "theme-change": "^2.3.0"
  },
  "keywords": [
    "Babel",
    "Boilerplate",
    "ES6",
    "ESLint",
    "Jest",
    "JavaScript",
    "PostCSS",
    "Stylelint",
    "Template",
    "TypeScript",
    "Webpack 5"
  ]
}
