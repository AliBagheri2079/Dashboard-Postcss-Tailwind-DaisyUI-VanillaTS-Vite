{
  "name": "dashboard-ts",
  "title": "Dashboard Typescript Project",
  "description": "This Project Template built with TypeScript/JavaScript, Babel, Vite, Webpack 5, PostCSS, Jest, ESLint, Stylelint and ...",
  "author": "Ali Bagheri",
  "homepage": "https://AliBagheri2079.github.io/dashboard-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:scripts": "eslint --ext .ts,.js src/ --fix",
    "lint:styles": "stylelint ./src/**/*.css",
    "lint": "npm run lint:scripts && npm run lint:styles",
    "format:scripts": "prettier . --write",
    "format:styles": "stylelint ./src/**/*.css --fix",
    "format": "npm run format:scripts && npm run format:styles",
    "test": "jest --verbose",
    "test:watch": "npm run test -- --watchAll",
    "test:coverage": "npm run test -- --coverage",
    "prepare": "husky install",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.15",
    "daisyui": "^2.50.1",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^13.1.2",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^8.0.1",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "stylelint": "^15.1.0",
    "stylelint-config-standard": "^30.0.1",
    "tailwindcss": "^3.2.7",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.0"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "chart.js": "^4.2.1",
    "firebase": "^9.18.0",
    "theme-change": "^2.3.0"
  },
  "keywords": [
    "Babel",
    "Boilerplate",
    "ES6",
    "ESLint",
    "Jest",
    "JavaScript",
    "PostCSS",
    "Stylelint",
    "Template",
    "TypeScript",
    "Webpack 5"
  ]
}
